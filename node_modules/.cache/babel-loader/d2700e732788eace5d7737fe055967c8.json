B,gBAAA,KAAkB,EAAlB;AAAA,2BAAA;AAAA;AAA8B,eAA5D,E,IAAA,E;;AAAA,gB,cAAA,CAA4D,CAA5D;AAAA,a,CADsB;;;;AAAA,WAA1B,CAEe,EACf,YAAA,CAEe,uBAFf,EAEe;AAFD,YAAA,KAAK,EAAC;AAEL,WAFf,EAAwB;8BACpB;AAAA,qBAA6D,CAA7D,YAAA,CAA6D,mBAA7D,EAA6D;4BAA1C,KAAA,CAAA,IAAA,CAAK,KAAqC;;yBAA1C,KAAA,CAAA,IAAA,CAAK,K,GAAK,M;kBAAgC;AAA9B,gBAAA,KAAkB,EAAlB;AAAA,2BAAA;AAAA;AAA8B,eAA7D,E,IAAA,E;;AAAA,gB,cAAA,CAA6D,CAA7D;AAAA,a,CADoB;;;;AAAA,WAAxB,CADe,EAKf,YAAA,CAEe,uBAFf,EAEe;AAFD,YAAA,KAAK,EAAC;AAEL,WAFf,EAAwB;8BACpB;AAAA,qBAA8D,CAA9D,YAAA,CAA8D,mBAA9D,EAA8D;4BAA3C,KAAA,CAAA,IAAA,CAAK,MAAsC;;yBAA3C,KAAA,CAAA,IAAA,CAAK,M,GAAM,M;kBAAgC;AAA9B,gBAAA,KAAkB,EAAlB;AAAA,2BAAA;AAAA;AAA8B,eAA9D,E,IAAA,E;;AAAA,gB,cAAA,CAA8D,CAA9D;AAAA,a,CADoB;;;;AAAA,WAAxB,CALe,EASf,YAAA,CAEe,uBAFf,EAEe;AAFD,YAAA,KAAK,EAAC;AAEL,WAFf,EAA0B;8BACtB;AAAA,qBAAwG,CAAxG,YAAA,CAAwG,yBAAxG,EAAwG;4BAA/E,KAAA,CAAA,IAAA,CAAK,IAA0E;;yBAA/E,KAAA,CAAA,IAAA,CAAK,I,GAAI,M;kBAAsE;AAApE,gBAAA,IAAI,EAAC,MAA+D;AAAxD,gBAAA,MAAM,EAAC,YAAiD;AAApC,gBAAA,KAAkB,EAAlB;AAAA,2BAAA;AAAA;AAAoC,eAAxG,E,IAAA,E;;AAAA,gB,cAAA,CAAwG,CAAxG;AAAA,a,CADsB;;;;AAAA,WAA1B,CATe,CAFf;AAAA,S;;;;OADJ,E;;AAAA,Q,SAAA,CAgBU,CAhBV;AAAA,K;;;;GADJ,E;;AAAA,I,cAAA,CA2BE,CAtCN,CAwCE,CAtFN,C","sourcesContent":["<template>\r\n    <div style=\"padding: 10px\">\r\n\r\n        <!--      功能区域-->\r\n        <div style=\"margin: 10px 0 ;\">\r\n            <el-button type=\"primary\" @click=\"add\">新增</el-button>\r\n            <el-button type=\"primary\">导入</el-button>\r\n            <el-button type=\"primary\">导出</el-button>\r\n        </div>\r\n\r\n        <!--      搜索区域-->\r\n        <div style=\"margin: 10px 0\">\r\n            <el-input v-model=\"search\" placeholder=\"输入关键字\" style=\"width: 20%\" clearable/>\r\n            <el-button type=\"primary\" style=\"margin-left: 5px\" @click=\"load\">查询</el-button>\r\n        </div>\r\n\r\n        <!--      表单-->\r\n        <el-table :data=\"tableData\"\r\n                  :header-cell-style=\"{'text-align':'center'}\"\r\n                  :cell-style=\"{'text-align':'center'}\"\r\n                  border\r\n                  stripe\r\n                  style=\"width: 100%;\">\r\n            <el-table-column prop=\"id\" label=\"ID\"/>\r\n            <el-table-column prop=\"name\" label=\"书籍名称\"/>\r\n            <el-table-column prop=\"price\" label=\"单价\"/>\r\n\r\n            <el-table-column prop=\"author\" label=\"作者\"/>\r\n            <el-table-column prop=\"time\" label=\"出版时间\"/>\r\n\r\n            <el-table-column label=\"选项\" width=\"200\">\r\n                <!--<el-table-column fixed=\"right\" label=\"选项\" width=\"200\">-->\r\n\r\n                <template #default=\"scope\">\r\n\r\n                    <el-button size=\"small\" type=\"text\" @click=\"handleEdit(scope.row)\">编辑</el-button>\r\n\r\n                    <el-popconfirm title=\"确认删除吗？\" @confirm=\"handleDelete(scope.row.id)\">\r\n                        <template #reference>\r\n                            <el-button size=\"small\" type=\"danger\">删除</el-button>\r\n                        </template>\r\n                    </el-popconfirm>\r\n\r\n                </template>\r\n            </el-table-column>\r\n        </el-table>\r\n\r\n        <div style=\"margin: 10px 0\">\r\n            <el-pagination\r\n                    v-model:currentPage=\"currentPage\"\r\n                    :page-sizes=\"[5, 10, 20]\"\r\n                    :page-size=\"pageSize\"\r\n                    layout=\"total, sizes, prev, pager, next, jumper\"\r\n                    :total=\"total\"\r\n                    @size-change=\"handleSizeChange\"\r\n                    @current-change=\"handleCurrentChange\">\r\n            </el-pagination>\r\n\r\n            <el-dialog v-model=\"dialogVisible\" title=\"书籍信息\" width=\"30%\">\r\n                <el-form :model=\"form\" label-width=\"120px\">\r\n                    <el-form-item label=\"书籍名称\">\r\n                        <el-input v-model=\"form.name\" style=\"width: 80%\"></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"价格\">\r\n                        <el-input v-model=\"form.price\" style=\"width: 80%\"></el-input>\r\n                    </el-form-item>\r\n\r\n                    <el-form-item label=\"作者\">\r\n                        <el-input v-model=\"form.author\" style=\"width: 80%\"></el-input>\r\n                    </el-form-item>\r\n\r\n                    <el-form-item label=\"出版时间\">\r\n                        <el-date-picker v-model=\"form.time\" type=\"date\" format=\"YYYY/MM/DD\" style=\"width: 80%\"></el-date-picker>\r\n                    </el-form-item>\r\n\r\n                </el-form>\r\n\r\n                <template #footer>\r\n                      <span class=\"dialog-footer\">\r\n                        <el-button @click=\"dialogVisible = false\">取消</el-button>\r\n                        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n                      </span>\r\n                </template>\r\n            </el-dialog>\r\n\r\n        </div>\r\n\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\n\r\n    import request from \"../utils/request\";\r\n\r\n    export default {\r\n        name: 'Book',\r\n        components: {},\r\n        data() {\r\n            return {\r\n                // 从后台获取数据\r\n                form: {},\r\n                dialogVisible: false,\r\n                search: '',\r\n                currentPage: 1,\r\n                pageSize: 10,\r\n                total: 0,\r\n\r\n                tableData: []\r\n            }\r\n        },\r\n\r\n        created() {\r\n            this.load();\r\n\r\n        },\r\n\r\n        methods: {\r\n            // 函数\r\n            load() {\r\n                request.get(\"/book\", {\r\n                    params: {\r\n                        pageNum: this.currentPage,\r\n                        pageSize: this.pageSize,\r\n                        search: this.search\r\n                    }\r\n                }).then(res => {\r\n                    console.log(res);\r\n                    this.tableData = res.data.records;\r\n                    this.total = res.data.total;\r\n                })\r\n            },\r\n            add() {\r\n                this.dialogVisible = true;\r\n                // 清空表单域\r\n                this.form = {}\r\n            },\r\n            save() {\r\n                if (this.form.id) {\r\n                    //更新\r\n                    request.put(\"/book\", this.form).then(res => {\r\n                        console.log(res);\r\n                        if (res.code === '0') {\r\n                            this.$message.success(\"更新成功\")\r\n                        } else {\r\n                            this.$message({\r\n                                type: \"error\",\r\n                                message: res.msg\r\n                            })\r\n                        }\r\n                        this.load();\r\n                        //刷新表格数据\r\n                        this.dialogVisible = false\r\n                    })\r\n                } else {\r\n                    // 新增\r\n                    request.post(\"/book\", this.form).then(res => {\r\n                        console.log(res);\r\n                        if (res.code === '0') {\r\n                            this.$message.success(\"更新成功\")\r\n                        } else {\r\n                            this.$message({\r\n                                type: \"error\",\r\n                                message: res.msg\r\n                            })\r\n                        }\r\n                        this.load();\r\n                        //刷新表格数据\r\n                        this.dialogVisible = false\r\n                    })\r\n                }\r\n\r\n            },\r\n            handleEdit(row) {\r\n                this.form = JSON.parse(JSON.stringify(row));\r\n                this.dialogVisible = true\r\n\r\n            },\r\n            handleSizeChange(pageSize) {\r\n                // 改变当前页数出发\r\n                this.pageSize = pageSize;\r\n                this.load()\r\n\r\n            },\r\n            handleCurrentChange(pageNum) {\r\n                // 改变当前每页个数出发\r\n                this.currentPage = pageNum;\r\n                this.load()\r\n\r\n            },\r\n            handleDelete(id) {\r\n                console.log(id);\r\n\r\n                request.delete(\"/book/\" + id).then(res => {\r\n\r\n                        if (res.code === '0') {\r\n                            this.$message.success(\"删除成功\")\r\n                        } else {\r\n                            this.$message({\r\n                                type: \"error\",\r\n                                message: res.msg\r\n                            })\r\n                        }\r\n                        this.load()\r\n                    }\r\n                )\r\n            },\r\n\r\n        }\r\n    }\r\n</script>\r\n"],"sourceRoot":""}]}